{
    "name": "calambago/calambago-backend",
    "description": "Backend for the CalambaGO! application",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "illuminate/database": "^8.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}